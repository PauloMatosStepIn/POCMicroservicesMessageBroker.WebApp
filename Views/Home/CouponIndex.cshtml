@model IEnumerable<CouponDto>
<form>
    <div class="container row">

        @foreach(var coupon in Model){

            <div class="col-12 col-md-6 col-lg-4">
            <div class="p-0 my-3 border">
                <div class="card">
                    <div class="card-body">
                        <h3 class="card-title text-success">@coupon.CouponCode</h3>
                        <div class="row">
                            <div class="col pt-1">
                                    <span style="font-size:23px;">Asked in : @coupon.CouponDate.ToString("yyyy/MM/dd hh:mm:ss tt")</span>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
        }
    </div>
</form>
